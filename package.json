{
  "name": "weex-debugger",
  "description": "weex developer tool for debugging weex app with chrome devtool",
  "version": "1.0.6",
  "author": "erha19 <faterrole@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "weex",
    "debugger",
    "inspector",
    "devtool"
  ],
  "scripts": {
    "lint": "eslint src --fix",
    "dev": "babel src -w --out-dir lib",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run lint && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^2.13.1"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "boxen": "^1.2.2",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "css-loader": "^0.28.7",
    "del": "^2.2.1",
    "detect-port": "^1.2.1",
    "exit": "^0.1.2",
    "fs-extra": "^5.0.0",
    "invok": "^1.0.2",
    "ip": "^1.1.3",
    "ipromise": "^1.0.2",
    "javascript-obfuscator": "^0.8.6",
    "koa": "^1.2.0",
    "koa-bodyparser": "^2.3.0",
    "koa-router": "~5.2.3",
    "koa-serve-static": "0.0.1",
    "koa-websocket": "^2.0.0",
    "mkdirp": "^0.5.1",
    "node-watch": "^0.3.5",
    "opn": "^4.0.2",
    "parse-hosts": "^1.0.1",
    "puppeteer": "^1.0.0",
    "request": "^2.83.0",
    "simrun": "^0.1.0",
    "webpack": "^1.13.1",
    "weex-builder": "^0.3.14",
    "weex-components": "^0.2.0",
    "weex-loader": "^0.4.2-beta",
    "weex-transformer": "^0.3.1",
    "ws": "^2.2.0"
  },
  "bin": {
    "weex-debugger": "bin/weex-devtool.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
}
